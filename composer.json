{
    "name": "ddebin/mc-google-visualization",
    "type": "library",
    "description": "Google Visualization datasource with your own database",
    "keywords": [
        "google",
        "visualization",
        "charts",
        "datasource",
        "database"
    ],
    "homepage": "https://github.com/ddebin/mc-google-visualization",
    "license": "MIT",
    "authors": [
        {
            "name": "Chadwick Morris",
            "email": "chad@mailchimp.com"
        },
        {
            "name": "Damien Debin",
            "email": "damien.debin@gmail.com"
        }
    ],
    "require": {
        "php": "^5.4 || ^7.0",
        "ext-json": "*",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4"
    },
    "autoload": {
        "psr-4": {
            "MC\\": "lib/MC"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "platform": {
            "php": "5.4"
        }
    }
}
