{
    "name": "ddebin/mc-google-visualization",
    "type": "library",
    "description": "Google Visualization datasource with your own database",
    "keywords": [
        "google",
        "visualization",
        "charts",
        "datasource",
        "database"
    ],
    "homepage": "https://github.com/ddebin/mc-google-visualization",
    "license": "MIT",
    "authors": [
        {
            "name": "Chadwick Morris",
            "email": "chad@mailchimp.com"
        },
        {
            "name": "Damien Debin",
            "email": "damien.debin@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-json": "*",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0",
        "phpstan/phpstan": "^0.9|^0.10|^0.11",
        "phpstan/phpstan-phpunit": "^0.9|^0.10|^0.11"
    },
    "autoload": {
        "psr-4": {
            "MC\\": "lib/MC"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "phpunit": "phpunit",
        "phpstan": "phpstan analyse -l max lib tests"
    }
}
